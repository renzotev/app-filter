@font-face {
    font-family: 'catalogo';
    src: url('../fonts/catalogo-webfont.eot');
    src: url('../fonts/catalogo-webfont.eot?#iefix') format('embedded-opentype'),
         url('../fonts/catalogo-webfont.woff') format('woff'),
         url('../fonts/catalogo-webfont.ttf') format('truetype'),
         url('../fonts/catalogo-webfont.svg#catalogoregular') format('svg');
    font-weight: normal;
    font-style: normal;

}


@font-face {
    font-family: 'catalogo';
    src: url('../fonts/catalogo_semigruesa-webfont.eot');
    src: url('../fonts/catalogo_semigruesa-webfont.eot?#iefix') format('embedded-opentype'),
         url('../fonts/catalogo_semigruesa-webfont.woff') format('woff'),
         url('../fonts/catalogo_semigruesa-webfont.ttf') format('truetype'),
         url('../fonts/catalogo_semigruesa-webfont.svg#catalogo_semigruesaregular') format('svg');
    font-weight: bold;
    font-style: normal;

}

* {
 -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
}
:focus {
    outline: 0;
    border:none;
    color: rgba(0, 0, 0, 0);
}
