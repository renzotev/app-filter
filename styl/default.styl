html,button,input,select,textarea {
	color:#222
}

hr {
	display:block;
	height:1px;
	border:0;
	border-top:1px solid #ccc;
	margin:1em 0;
	padding:0
}
img {
	vertical-align:middle
}
fieldset {
	border:0;
	margin:0;
	padding:0
}
textarea {
	resize:vertical
}
select {
	margin:0;
	padding:0;
	outline:none;
	font-family:inherit;	
	-moz-box-sizing:border-box; /* Firefox */
	-webkit-box-sizing:border-box; /* Safari */
	box-sizing:border-box;
}
.chromeframe {
	margin:.2em 0;
	background:#ccc;
	color:#000;
	padding:.2em 0
}
